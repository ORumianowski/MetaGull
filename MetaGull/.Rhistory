library(tidyverse)
path_dataset_gull = "C:/Users/33763/Documents/CMRi/0_Github/Z_0_Dataset/Peron_dataset_0502024/public-colonie.csv"
survey = read.csv(path_dataset_gull) %>%
dplyr::select(annee_observation_colonie,
id_site_mesure,
nombre_couple_colonie) %>%
rename(year = annee_observation_colonie,
site = id_site_mesure,
N = nombre_couple_colonie)
survey = survey %>%
subset(., year %in% 1986:2005)
survey_with_cmr = survey %>%
subset(.,site %in% c("LR_E", "MA_E", "PC_E", "V5_E")) %>%
pivot_wider(names_from = year, values_from = N)
survey_with_cmr = survey_with_cmr %>%
slice(c(1, 3, 2, 4)) %>% # Obtenir l'ordre : 1-LR, 2-PC, 3-MA, 4-V5  (le même que celui de la CMR)
select(-1)
alive_elsewhere = survey %>%
subset(.,!(site %in% c("LR_E", "MA_E", "PC_E", "V5_E")))%>%
pivot_wider(names_from = year, values_from = N)%>%
select(-1) %>%
colSums(.,na.rm = T)
B = rbind(survey_with_cmr, alive_elsewhere) %>%
as.matrix()
# Nombre de couple issu de la thèse de V.GROBOIS
B[1,] = c(7145, 8059, 5585, 7893, 8487, 10428, 10877, 7310, 5882, 6027, 6584, 8499, 9225, 6639, 7000, 7000, 7000, 7000, 7000, 7000)%/%2
# Année d'inction issu de Péron
B[2, c(9, 19:20)] = 0
B[3, c(7, 17)] = 0
B[4, c(1:5, 7:10, 16:20)] = 0
save(B, file = "B.Rda")
# Plot --------------------------------------------------------------------
data_ = as.matrix(B)
df <- data.frame(
Temps = rep(seq_len(ncol(data_)), each = nrow(data_)),
Population = rep(c("pop1", "pop2", "pop3", "pop4", "pop5"), time = ncol(data_)),
Valeur = as.vector(data_))
ggplot(df, aes(x = Temps, y = Valeur, color = Population)) +
geom_line() +
labs(title = "Effectifs des 5 populations - données réelles",
x = "Temps",
y = "Nombre d'individus") +
theme_minimal()
